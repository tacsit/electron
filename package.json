{
    "name": "mpp",
    "version": "1.0.0",
    "description": "An Electron application with React",
    "main": "./.electron/out/main/index.js",
    "author": "example.com",
    "homepage": "https://www.electronjs.org",
    "license": "MIT",
    "scripts": {
        "dev": "electron-vite dev --config .config/electron.vite.config.js",
        "build": "electron-vite build --config .config/electron.vite.config.js",
        "build:win": "npm run build && electron-builder --win --config=.config/electron-builder.yml",
        "build:mac": "npm run build && electron-builder --mac --config=.config/electron-builder.yml",
        "build:linux": "npm run build && electron-builder --linux --config=.config/electron-builder.yml",
        "preview": "electron-vite preview --config .config/electron.vite.config.js",
        "postinstall": "electron-builder install-app-deps"
    },
    "dependencies": {
        "@electron-toolkit/preload": "^1.0.3",
        "@electron-toolkit/utils": "^1.0.2",
        "electron-updater": "^5.3.0"
    },
    "devDependencies": {
        "@electron/notarize": "^1.2.3",
        "@headlessui/react": "^1.7.13",
        "@headlessui/tailwindcss": "^0.1.2",
        "@heroicons/react": "^2.0.17",
        "@vitejs/plugin-react": "^3.1.0",
        "autoprefixer": "^10.4.14",
        "electron": "^22.3.4",
        "electron-builder": "^23.6.0",
        "electron-vite": "^1.0.21",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "hotkeys-js": "^3.10.2",
        "ol": "^7.3.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-feather": "^2.0.10",
        "tailwind-scrollbar": "^3.0.0",
        "tailwindcss": "^3.3.0",
        "ts-key-enum": "^2.0.12",
        "usehooks-ts": "^2.9.1",
        "vite": "^4.2.1"
    }
}
